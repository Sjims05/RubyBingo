<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="hitBlue">#08B4E7</Color>
    <Color x:Key="hitYellow">#E7C600</Color>

    <SolidColorBrush Color="{StaticResource hitBlue}" x:Key="Hit-Blue"/>
    <SolidColorBrush Color="{StaticResource hitYellow}" x:Key="Hit-Yellow"/>
    <SolidColorBrush Color="#909194" x:Key="Hit-White"/>
    <SolidColorBrush Color="#606165" x:Key="Hit-Grey"/>

</ResourceDictionary>